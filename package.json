{
  "name": "foreach-prop",
  "version": "2.0.0",
  "description": "Array-like methods for objects",
  "main": "dist/each-prop.cjs.js",
  "module": "dist/each-prop.es.js",
  "browser": "dist/each-prop.umd.js",
  "types": "types/index.d.ts",
  "bundlib": {
    "name": "eachProp",
    "min": "browser"
  },
  "keywords": [
    "for-in",
    "for-each",
    "object",
    "properties",
    "property",
    "key",
    "keys",
    "filter",
    "reduce",
    "map"
  ],
  "files": [
    "dist",
    "types",
    "src"
  ],
  "scripts": {
    "lint": "tslint src/**/*.ts",
    "clean": "rm -rf .cache dist types coverage",
    "dev": "bundlib -dw",
    "build": "npm run lint && bundlib -d",
    "test": "jest --colors",
    "preversion": "npm run clean && npm run build && npm test"
  },
  "dependencies": {
    "args-to-arr": "^1.3.13",
    "is-function": "^1.0.1"
  },
  "devDependencies": {
    "@types/is-function": "^1.0.0",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.4",
    "bundlib": "^0.5.0",
    "eslint": "^5.16.0",
    "jest": "^24.8.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1"
  },
  "author": {
    "name": "Manuel Fern√°ndez",
    "email": "manferlo81@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manferlo81/foreach-prop.git"
  },
  "bugs": {
    "url": "https://github.com/manferlo81/foreach-prop/issues"
  },
  "homepage": "https://github.com/manferlo81/foreach-prop#readme",
  "license": "MIT"
}
