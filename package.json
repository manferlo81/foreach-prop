{
  "name": "foreach-prop",
  "version": "2.0.7",
  "description": "Array-like methods for objects",
  "keywords": [
    "for-in",
    "for-each",
    "object",
    "properties",
    "property",
    "key",
    "keys",
    "filter",
    "reduce",
    "map"
  ],
  "main": "dist/each-prop.js",
  "module": "dist/each-prop.mjs",
  "browser": "dist/each-prop.umd.js",
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "bundlib": {
    "name": "eachProp",
    "min": "browser"
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "bundlib -dw",
    "build": "bundlib -d",
    "test": "jest --color -w 3",
    "clean": "rimraf node_modules/.cache/bundlib node_modules/.cache/jest dist types coverage",
    "lint": "eslint src/**/*.ts __test__/**/*.ts",
    "prerelease": "npm run clean && npm run lint && npm test && npm run build",
    "release": "standard-version"
  },
  "dependencies": {
    "args-to-arr": "^1.4.8",
    "is-function": "^1.0.1"
  },
  "devDependencies": {
    "@types/is-function": "^1.0.0",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.23.0",
    "bundlib": "^0.14.4",
    "chokidar": "^3.3.1",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "rimraf": "^3.0.2",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "author": {
    "name": "Manuel Fern√°ndez",
    "email": "manferlo81@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manferlo81/foreach-prop.git"
  },
  "bugs": {
